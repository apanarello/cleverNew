<?xml version="1.0"?>
<!-- Smack configuration file. -->
<smack>

    <!-- Classes that will be loaded when Smack starts -->
    <startupClasses>
        <className>org.clever.Common.smackx.ServiceDiscoveryManager</className>
        <className>org.clever.Common.smack.PrivacyListManager</className>        
        <className>org.clever.Common.smackx.XHTMLManager</className>
        <className>org.clever.Common.smackx.muc.MultiUserChat</className>
        <className>org.clever.Common.smackx.bytestreams.ibb.InBandBytestreamManager</className>
        <className>org.clever.Common.smackx.bytestreams.socks5.Socks5BytestreamManager</className>    
        <className>org.clever.Common.smackx.filetransfer.FileTransferManager</className>
        <className>org.clever.Common.smackx.LastActivityManager</className>
        <className>org.clever.Common.smack.ReconnectionManager</className>    
        <className>org.clever.Common.smackx.commands.AdHocCommandManager</className>    
    </startupClasses>

    <!-- Packet reply timeout in milliseconds -->
    <packetReplyTimeout>5000</packetReplyTimeout>

    <!-- Keep-alive interval in milliseconds -->
    <keepAliveInterval>30000</keepAliveInterval>

    <!-- Enable/Disable local Socks5 proxy -->
    <localSocks5ProxyEnabled>true</localSocks5ProxyEnabled>

    <!-- Port of the local Socks5 proxy -->
    <localSocks5ProxyPort>7777</localSocks5ProxyPort>

    <!-- Port of the local Socks5 proxy -->
    <packetCollectorSize>10000</packetCollectorSize>

</smack>
